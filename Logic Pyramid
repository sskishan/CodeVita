t=int(input())
for _ in range(t):
    x=6
    y=22
    n=int(input())
    k=n 
    for i in range(n):
        for j in range(1,k):
            print(end=" "*3)
        k-=1
        for j in range(0,i+1):
            print("{:0>5d}".format(x),end=" ")
            x=x+y 
            y=y+16
        print()

        




